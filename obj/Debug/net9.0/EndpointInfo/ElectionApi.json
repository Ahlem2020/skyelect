{
  "openapi": "3.0.1",
  "info": {
    "title": "ElectionApi",
    "version": "1.0"
  },
  "paths": {
    "/candidates": {
      "get": {
        "tags": [
          "Candidate"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/candidates/stats/perHours": {
      "get": {
        "tags": [
          "Candidate"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/candidates/stats/perMinutes": {
      "get": {
        "tags": [
          "Candidate"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/offices": {
      "get": {
        "tags": [
          "Office"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      },
      "post": {
        "tags": [
          "Office"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddOfficeViewModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddOfficeViewModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AddOfficeViewModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/votes": {
      "post": {
        "tags": [
          "Vote"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddVoteViewModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AddVoteViewModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AddVoteViewModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AddOfficeViewModel": {
        "required": [
          "id",
          "registered"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "offices": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "code": {
            "type": "string",
            "nullable": true
          },
          "registered": {
            "type": "integer",
            "format": "int64"
          },
          "province": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AddVoteViewModel": {
        "required": [
          "candidates",
          "office",
          "password"
        ],
        "type": "object",
        "properties": {
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "office": {
            "type": "integer",
            "format": "int32"
          },
          "candidates": {
            "minItems": 1,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CandidateVoteViewModel"
            }
          }
        },
        "additionalProperties": false
      },
      "CandidateVoteViewModel": {
        "required": [
          "id"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "vote": {
            "type": "integer",
            "format": "int64",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}